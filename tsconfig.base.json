{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "importHelpers": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ES2022", "ESNext.Array", "DOM", "ES2020.Intl"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@li/config/design": ["libs/config/design/src/index.ts"],
      "@li/config/options": ["libs/config/options/src/index.ts"],
      "@li/config/utils": ["libs/config/utils/src/index.ts"],
      "@li/design/components": ["libs/design/components/src/index.ts"],
      "@li/design/elements": ["libs/design/elements/src/index.ts"],
      "@li/design/enhancers": ["libs/design/enhancers/src/index.ts"],
      "@li/design/hooks": ["libs/design/hooks/src/index.ts"],
      "@li/design/icons": ["libs/design/icons/src/index.ts"],
      "@li/types/config": ["libs/types/config/src/index.ts"],
      "@li/types/design": ["libs/types/design/src/index.ts"],
      "@li/types/shared": ["libs/types/shared/src/index.ts"],
      "@md/blaunk/config": ["modules/blaunk/config/src/index.ts"],
      "@md/blaunk/design": ["modules/blaunk/design/src/index.ts"],
      "@md/blaunk/pages/auth": ["modules/blaunk/pages/auth/src/index.ts"],
      "@md/blaunk/pages/b2b": ["modules/blaunk/pages/b2b/src/index.ts"],
      "@md/blaunk/pages/home": ["modules/blaunk/pages/home/src/index.ts"],
      "@md/blaunk/pages/layout": ["modules/blaunk/pages/layout/src/index.ts"],
      "@md/blaunk/types": ["modules/blaunk/types/src/index.ts"]
    }
  },
  "files": [],
  "exclude": ["node_modules", "tmp"]
}
